#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int x1,i; 
    int v1; 
    int x2; 
    int v2; 
    scanf("%d %d %d %d",&x1,&v1,&x2,&v2);
    if(v1>=1 && v2>=1 && v1<=10000 && v2<=10000)
    for(i=0;i<1000;i++)
        if(x1!=x2)
        {
    x1=x1+v1;
    x2=x2+v2;
}
    if(x1==x2)
        printf("YES");
    else
        printf("NO");
    
    return 0;
}
