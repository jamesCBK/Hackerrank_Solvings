#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int n,z=1,i,j,k; 
    scanf("%d",&n);
for(k=1;k<=n;k++)
     {
    for(j=n-1;j>=k;j--)
       {      
        printf(" ");
    }
    for(i=1;i<=z;i++)
        {
        printf("#");
    }
       z++;
        printf("\n");
    }
    return 0;
}
