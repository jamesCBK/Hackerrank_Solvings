#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char s[100],p[100];
    int n,i,count=0,k,c=0 ;
        scanf("%s",s);
    n=strlen(s);
    if((n>1) && (n<=100)){
    for(i='a';i<'z';i++)
    {
        count=0;
        for(k=0;k<n;k++)
    {
            if(i==s[k])
                {
                count++;} 
           
    } c+=count;
      
                    if(count%2!=0)
                    {
                        printf("%c",i);
                    }
 
     
                    }if(c%2==0){printf("Empty String");}
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
